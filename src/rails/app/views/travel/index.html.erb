<script type="text/javascript" src="/javascripts/airport_autocomplete.js" language="javascript" charset="utf-8"></script>

<h1>Travel App</h1>

<%= semantic_form_for :travel, :url => '/travel' do |f| %>
  <%= f.inputs do %>
    <%= f.input :from %>
    <%= f.input :to %>
  <% end %>
  <%= f.submit 'Fly' %>
<% end %>
